<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="Aura.UI.ControlsGallery.Pages.CardControlPage"
             Classes="us_c">
	<UserControl.Styles>
		<Style Selector="CodeExample">
			<Setter Property="CSharpVisible" Value="False"/>
		</Style>
	</UserControl.Styles>
	<Grid RowDefinitions="auto,10,auto,auto,20,auto,auto,20,auto,auto">
		<TextBlock Grid.Row="0" Text="The recommended use of CardControl is shows MultimediaMedia information, like Music, Movies, Books, etc."/> 
		<CodeExample Title="CardControl in Horizontal Disposition" Grid.Row="3">
			<CodeExample.Control>
				<Grid RowDefinitions="*,*,*">
					<Grid.Styles>
						<Style Selector="CardControl">
							<Setter Property="InternalPadding" Value="0"/>
							<Setter Property="Width" Value="200"/>
							<Setter Property="Height" Value="260"/>
						</Style>
						<Style Selector="CardControl > Border.toclip">
							<Setter Property="ClipToBounds" Value="True"/>
						</Style>
					</Grid.Styles>
					<!--Colbreakz-->
					<CardCollection Header="Colbreakz" TileMode="HorizontalDisposition">
						<CardControl Header="20000" SecondaryHeader="Colbreakz">
							<Border CornerRadius="7 0" Classes="toclip">
								<Image Source="avares://Aura.UI.ControlsGallery/Assets/20000.jpg"  Stretch="UniformToFill"/>
							</Border>
						</CardControl>
						<CardControl Header="40000" SecondaryHeader="Colbreakz">
							<Border CornerRadius="7 0" Classes="toclip">
								<Image Source="avares://Aura.UI.ControlsGallery/Assets/40000.jpg"  Stretch="UniformToFill"/>
							</Border>
						</CardControl>
						<CardControl Header="75000" SecondaryHeader="Colbreakz">
							<Border CornerRadius="7 0" Classes="toclip">
								<Image Source="avares://Aura.UI.ControlsGallery/Assets/75000.jpg"  Stretch="UniformToFill"/>
							</Border>
						</CardControl>
						<CardControl Header="You and Me" SecondaryHeader="Colbreakz">
							<Border CornerRadius="7 0" Classes="toclip">
								<Image Source="avares://Aura.UI.ControlsGallery/Assets/younme.jpg"  Stretch="UniformToFill"/>
							</Border>
						</CardControl>
					</CardCollection>
					<!--Krewella-->
					<CardCollection Grid.Row="1"
						Header="Krewella" TileMode="HorizontalDisposition">
						<CardControl SecondaryHeader="Fall Out Boys">
							<CardControl.Header>
								Alone Togheter
							</CardControl.Header>
							<Border CornerRadius="7 0" Classes="toclip">

								<Image Source="avares://Aura.UI.ControlsGallery/Assets/alonetogheterRM.jpg" Stretch="UniformToFill"/>

							</Border>
						</CardControl>
						<CardControl Header="Greenlights" SecondaryHeader="Krewella">
							<Border CornerRadius="7 0" Classes="toclip">

								<Image Source="avares://Aura.UI.ControlsGallery/Assets/greenlights.jpg"  Stretch="UniformToFill"/>

							</Border>
						</CardControl>
						<CardControl Header="TH2C" SecondaryHeader="Krewella">
							<Border CornerRadius="7 0" Classes="toclip">

								<Image Source="avares://Aura.UI.ControlsGallery/Assets/th2cdyro.jpg"  Stretch="UniformToFill"/>

							</Border>
						</CardControl>

					</CardCollection>
					<!--Getsix-->
					<CardCollection Grid.Row="2"
						Header="Getsix" TileMode="HorizontalDisposition">
						<CardControl Header="Nerve" SecondaryHeader="Getsix and Miss Lina">
							<Border CornerRadius="7 0" Classes="toclip">

								<Image Source="avares://Aura.UI.ControlsGallery/Assets/nerve.jpg"  Stretch="UniformToFill"/>

							</Border>
						</CardControl>
						<CardControl Header="In to the Fire" SecondaryHeader="Getsix and Miss Lina">
							<Border CornerRadius="7 0" Classes="toclip">

								<Image Source="avares://Aura.UI.ControlsGallery/Assets/in2thefire.jpg"  Stretch="UniformToFill"/>

							</Border>
						</CardControl>
					</CardCollection>
				</Grid>
			</CodeExample.Control>
