<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Aura.UI.Gallery.Pages.GroupBoxPage"
						 Classes="us_c">
	<StackPanel Spacing="20">
		<TextBlock Text="The GroupBox shows information to the user with a Header and a Content."/>
    <CodeExample Title="Standard GroupBox" CSharpVisible="False" HorizontalAlignment="Center">
			<GroupBox Header="Hello!" Content="I'm a GroupBox"/>
      <CodeExample.XAMLText>
 &lt;GroupBox Header="Hello!" Content="I'm a GroupBox"/>        
      </CodeExample.XAMLText>
		</CodeExample>
	</StackPanel>
</UserControl>
