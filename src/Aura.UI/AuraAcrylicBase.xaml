<Styles xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Styles.Resources>
		<ExperimentalAcrylicMaterial x:Key="NullBrush"  MaterialOpacity="-10" TintOpacity="0"
											  TintColor="#00000000"
									 FallbackColor="#00000000"></ExperimentalAcrylicMaterial>
		<ExperimentalAcrylicMaterial x:Key="OnlyNoiseBrush"  MaterialOpacity="0.5" TintOpacity="0"
							 TintColor="{DynamicResource ThemeBackgroundColor}"
							 FallbackColor="{DynamicResource ThemeBackgroundColor}"
									 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicBackgroundBrush"  MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeBackgroundColor}"
								 FallbackColor="{DynamicResource ThemeBackgroundColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicBorderLowBrush"   MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeBorderLowColor}"
								 FallbackColor="{DynamicResource ThemeBorderLowColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicBorderMidBrush"		MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeBorderMidColor}"
								 FallbackColor="{DynamicResource ThemeBorderMidColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicBorderHighBrush"	MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeBorderHighColor}"
								 FallbackColor="{DynamicResource ThemeBorderHighColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicControlLowBrush"	MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeControlLowColor}"
								 FallbackColor="{DynamicResource ThemeControlLowColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicControlMidBrush"    MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeControlMidColor}"
								 FallbackColor="{DynamicResource ThemeControlMidColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicControlMidHighBrush"	MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeControlMidHighColor}"
								 FallbackColor="{DynamicResource ThemeControlMidHighColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicControlHighBrush"	MaterialOpacity="0" TintOpacity="0.5"
								 	 TintColor="{DynamicResource ThemeControlHighColor}"
								 FallbackColor="{DynamicResource ThemeControlHighColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicControlVeryHighBrush"	MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeControlVeryHighColor}"
							 FallbackColor="{DynamicResource ThemeControlVeryHighColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicControlHighlightLowBrush"	MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeControlHighlightLowColor}"
							 FallbackColor="{DynamicResource ThemeControlHighlightLowColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicControlHighlightMidBrush"	MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeControlHighlightMidColor}"
							 FallbackColor="{DynamicResource ThemeControlHighlightMidColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicControlHighlightHighBrush"	MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeControlHighlightHighColor}"
							 FallbackColor="{DynamicResource ThemeControlHighlightHighColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicForegroundBrush"	MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeForegroundColor}"
							 FallbackColor="{DynamicResource ThemeForegroundColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicForegroundLowBrush"	MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeForegroundLowColor}"
							 FallbackColor="{DynamicResource ThemeForegroundLowColor}"
								 BackgroundSource="Digger"/>

		<ExperimentalAcrylicMaterial x:Key="HighlightAcrylicBrush"	MaterialOpacity="0" TintOpacity="0.5" 
									 TintColor="{DynamicResource HighlightColor}"
						 FallbackColor="{DynamicResource HighlightColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="HighlightForegroundBrush"	MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource HighlightForegroundColor}"
						 FallbackColor="{DynamicResource HighlightForegroundColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicAccentBrush"	MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeAccentColor}"
							FallbackColor="{DynamicResource ThemeAccentColor}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicAccentBrush2"	MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeAccentColor2}"
						FallbackColor="{DynamicResource ThemeAccentColor2}"
								 BackgroundSource="Digger"/>
		<ExperimentalAcrylicMaterial x:Key="ThemeAcrylicAccentBrush3"	MaterialOpacity="0" TintOpacity="0.5"
									 TintColor="{DynamicResource ThemeAccentColor3}"
						 FallbackColor="{DynamicResource ThemeAccentColor3}"
								 BackgroundSource="Digger"/>
