<Styles xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:sys="clr-namespace:System;assembly=mscorlib">
	<Design.PreviewWith>
		<Button></Button>
	</Design.PreviewWith>
	<StyleInclude Source="/AuraUIResources.Common.axaml"/>
	<Styles.Resources>
