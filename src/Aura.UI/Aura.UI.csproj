<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<Description>Control's Library for Avalonia.</Description>	  
		<PackageIcon>AuraUILogo_full_icon.png</PackageIcon>
		<GenerateAssemblyInfo>true</GenerateAssemblyInfo>
  </PropertyGroup>
	<ItemGroup>
		<None Include="..\..\.editorconfig" Link=".editorconfig" />
		<None Include="AuraUILogo_full_icon.png" Pack="true" PackagePath="\" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\Aura.UI.Dragging\Aura.UI.Dragging.csproj" />
		<AvaloniaResource Include="**\*.xaml">
			<SubType>Designer</SubType>
		</AvaloniaResource>
	  <AvaloniaResource Include="Assets\squares.png" />
	</ItemGroup>
	<Import Project="..\..\build\version.props" />
	<ItemGroup>
	  <PackageReference Include="Avalonia" Version="0.10.6" />
	  <PackageReference Include="Avalonia.Desktop" Version="0.10.8" />
	  <PackageReference Include="Avalonia.Diagnostics" Version="0.10.8" />
	  <PackageReference Include="Avalonia.Markup.Xaml.Loader" Version="0.10.8" />
	  <PackageReference Include="Avalonia.ReactiveUI" Version="0.10.8" />
	  <PackageReference Include="Avalonia.Xaml.Behaviors" Version="0.10.8" />
	  <PackageReference Include="Avalonia.Xaml.Interactivity" Version="0.10.8" />
	  <PackageReference Include="MathNet.Numerics" Version="4.15.0" />
	  <PackageReference Include="ReactiveUI" Version="16.2.6" />
	  <PackageReference Include="SkiaSharp" Version="2.80.3" />
	  <PackageReference Include="Avalonia" Version="0.10.8" />
	  <PackageReference Include="System.Reactive" Version="5.0.0" />
	</ItemGroup>
</Project>
